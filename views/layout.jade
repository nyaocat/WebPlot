doctype 5
html
  head
    title WebPlot
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    // Bootstrap
    link(href='/css/bootstrap.min.css', rel='stylesheet', media='screen')
    // HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries
    //if lt IE 9
    script(src='/assets/js/html5shiv.js')
    script(src='/assets/js/respond.min.js')
    // jQuery (necessary for Bootstrap's JavaScript plugins)
    script(src='/assets/js/jquery.js')
    script(src='/js/jquery.upload-1.0.2.js')
    script(src='/socket.io/socket.io.js')
    style(type='text/css')
      body { padding-top: 70px; }
  body
    header.navbar.navbar-inverse.navbar-fixed-top.bs-docs-nav(role='banner')
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.bs-navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') WebPlot
        nav.collapse.navbar-collapse.bs-navbar-collapse(role='navigation')
          ul.nav.navbar-nav
            - nav.forEach(function (o, i) {
              - if (i == current)
                li.active
                  a(href=o.path) #{o.str}
              - else
                li
                  a(href=o.path) #{o.str}
            - })


    block content